alias reload=". ~/.bashrc"
alias gs="git status"
alias pyvenv="pyvenv-3.5"
alias venv="pyvenv-3.5"
alias virtualenv="pyvenv-3.5"
alias ll="ls -la"
alias freeze="pip freeze"
alias llh="ls -lah"
alias vi="vim"
alias git-tag-rm="for el in $(git tag); do git tag -d ${el}; done"
alias git-clean-s="git clean -nXd ."
alias git-clean-f="git clean -fXd ."
alias dotfiles-commit="cd ~/dotfiles && git commit -am \"auto backup\" && git push"
alias dotfiles-commit="cd ~/dev/dotfiles && git commit -am \"auto backup\" && git push"
